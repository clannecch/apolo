<h1>Listado de Provincias</h1>

<table>
  <tr>
    <th>Detalle</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @provinces.each do |province| %>
  <tr>
    <td><%= province.detalle %></td>
    <td><%= link_to 'Visualizar', province %></td>
    <td><%= link_to 'Editar', edit_province_path(province) %></td>
    <td><%= link_to 'Borrar', province, :confirm => 'Esta Seguro?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nueva Provincia', new_province_path %>
