%h3 Aseguradora / Banco
.field
  = f.label "Aseguradora"
  %br/
  = f.collection_select :insurance_company_id, InsuranceCompany.all, :id, :detalle, :prompt => true
.field
  = f.label "Seguro Conyuge"
  %br/
  = f.text_field :seguro_conyuge
.field
  = f.label "Numero Poliza de Seguro"
  %br/
  = f.text_field :Seguro_numero_poliza
.field
  = f.label "Monto Asegurado"
  %br/
  = f.text_field :seguro_monto_asegurado
.field
  = f.label "Banco Deposito Haberes"
  %br/
  = f.collection_select :bank_deposit_id, BankDeposit.all, :id, :detalle, :prompt => true
.field
  = f.label "Tipo Cuenta Bancaria"
  %br/
  = f.collection_select :bank_deposit_account_type_id, BankDepositAccountType.all, :id, :detalle, :prompt => true
.field
  = f.label "Numero Cuenta Bancaria"
  %br/
  = f.text_field :deposito_banco_cuenta
.field
  = f.label "CBU Cuenta Bancaria"
  %br/
  = f.text_field :deposito_banco_cbu
