= form_for(@character_service) do |f|
  - if @character_service.errors.any?
    #error_explanation
      %h2
        = pluralize(@character_service.errors.count, "error")
        prohibited this character_service from being saved:
      %ul
        - @character_service.errors.full_messages.each do |msg|
          %li= msg
  %table.tableForm
    %tr
      %td.columnLabel
        = f.label :detalle, :class => "apsLabel"
      %td.columnData
        = f.text_field :detalle, :class => "apsTextbox"
  .actions
    = f.submit :class => "btn success"
