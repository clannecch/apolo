<h1>Menu Roles</h1>

<table>
	<tr>
		<th>Menu</th>
		<th>Rol</th>
		<th colspan='3' style="text-align:center;">Acciones</th>
	</tr>
	<% for menu_role in @menu_roles %>
		<tr>
			<td><%= menu_role.menu_id %></td>
			<td><%= menu_role.rol_id %></td>
            <td><a href="/menu_roles/<%= menu_role.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/menu_roles/<%= menu_role.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/menu_roles/<%= menu_role.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nuevo Menu Role", new_menu_role_path %></p>
