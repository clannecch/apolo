= form_for(@user) do |f|
  - if @user.errors.any?
    #error_explanation
      %h2
        = pluralize(@user.errors.count, "error")
        prohibited this user from being saved:
      %ul
        - @user.errors.full_messages.each do |msg|
          %li= msg
  .clear
  %br/
  .form_box
    .form_box_title
      %span.title= User.model_name.human
    %table.tableForm
      %tr
        %td.columnLabel
          = f.label :first_name, :class => "apsLabel"
        %td.columnData
          = f.text_field :first_name, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :last_name, :class => "apsLabel"
        %td.columnData
          = f.text_field :last_name, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :reset_password_token, :class => "apsLabel"
        %td.columnData
          = f.text_field :reset_password_token, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :reset_password_sent_at, :class => "apsLabel"
        %td.columnData
          = f.text_field :reset_password_sent_at, :class => "apsDatetime"
      %tr
        %td.columnLabel
          = f.label :remember_created_at, :class => "apsLabel"
        %td.columnData
          = f.text_field :remember_created_at, :class => "apsDatetime"
      %tr
        %td.columnLabel
          = f.label :current_sign_in_at, :class => "apsLabel"
        %td.columnData
          = f.text_field :current_sign_in_at, :class => "apsDatetime"
      %tr
        %td.columnLabel
          = f.label :last_sign_in_at, :class => "apsLabel"
        %td.columnData
          = f.text_field :last_sign_in_at, :class => "apsDatetime"
      %tr
        %td.columnLabel
          = f.label :current_sign_in_ip, :class => "apsLabel"
        %td.columnData
          = f.text_field :current_sign_in_ip, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :last_sign_in_ip, :class => "apsLabel"
        %td.columnData
          = f.text_field :last_sign_in_ip, :class => "apsTextbox"
  .actions
    = f.submit :class => "btn success"
