<h1>Tipo Recibos</h1>

<table class="tableStandard">
	<tr>
		<th><%= sort_link @search, :detalle, "Detalle" %></td>
		<th colspan='3' style="text-align:center;">Acciones</th>
	</tr>
	<% for tipo_recibo in @tipo_recibos %>
		<tr>
			<td><%= tipo_recibo.detalle %></td>
            <td><a href="/tipo_recibos/<%= tipo_recibo.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/tipo_recibos/<%= tipo_recibo.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/tipo_recibos/<%= tipo_recibo.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nuevo Tipo Recibo", new_tipo_recibo_path %></p>

<%= paginate @tipo_recibos %>
