= form_for(@insurance_company) do |f|
  - if @insurance_company.errors.any?
    #error_explanation
      %h2
        = pluralize(@insurance_company.errors.count, "error")
        prohibited this insurance_company from being saved:
      %ul
        - @insurance_company.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :detalle
    %br/
    = f.text_field :detalle
  .actions
    = f.submit
