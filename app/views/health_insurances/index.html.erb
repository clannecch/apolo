<h1>Obras Sociales</h1>

<table class="tableStandard">
	<tr>
        <th><%= sort_link @search, :Code, "Code" %></td>
        <th><%= sort_link @search, :detalle, "Detalle" %></td>
		<th colspan='3' style="text-align:center;">Acciones</th>
	</tr>
	<% for health_insurance in @health_insurances %>
		<tr>
			<td><%= health_insurance.code %></td>
            <td><%= health_insurance.detalle %></td>
            <td><a href="/health_insurances/<%= health_insurance.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/health_insurances/<%= health_insurance.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/health_insurances/<%= health_insurance.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nueva Obra Social", new_health_insurance_path %></p>

<%= paginate @health_insurances %>
