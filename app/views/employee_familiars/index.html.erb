<% @employee_familiars.each do |employee_familiar| %>
  <tr>
    <td><%= employee_familiar.apellido %></td>
    <td><%= employee_familiar.nombre %></td>
    <td><%= employee_familiar.fecha_nacimiento %></td>
    <td><%= employee_familiar.numero_documento %></td>
    <td><%= employee_familiar.health_insurance_id %></td>
    <td><%= employee_familiar.cuil %></td>
    <td><%= employee_familiar.cuit %></td>
    <td><%= employee_familiar.tomo %></td>
