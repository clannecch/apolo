    <td><%= employee_familiar.acta %></td>
    <td><%= employee_familiar.country_id %></td>
    <td><%= employee_familiar.province_id %></td>
    <td><%= employee_familiar.comuna %></td>
    <td><%= employee_familiar.document_type_id %></td>
    <td><%= employee_familiar.activity_id %></td>
    <td><%= employee_familiar.employee_id %></td>
    
    <td><%= link_to 'Visualizar', employee_familiar %></td>
    <td><%= link_to 'Editas', edit_employee_familiar_path(employee_familiar) %></td>
