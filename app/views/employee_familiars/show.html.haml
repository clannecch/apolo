- content_for(:breadcrumbs) do
  = breadcrumbs do |breadcrumb|
    - breadcrumb.item current_company.name.titlecase, root_path
    - breadcrumb.item EmployeeFamiliar.model_name.human, employee_familiars_path , :active => true

= link_to t('scaffold.actions.edit').titlecase, edit_employee_familiar_path(@employee_familiar), :class => "btn success" |
= link_to t('scaffold.actions.destroy').titlecase, @employee_familiar, :confirm => t('scaffold.actions.destroy_confirm'), :method => :delete, :class => "btn danger"
.clear
%br/
