= form_for(@insurance_beneficiary) do |f|
  = render 'shared/errorhandler', :obj_errors => @insurance_beneficiary.errors
  .clear
  .form_box
    .form_box_title
    /  %span.title= InsuranceBeneficiary.model_name.human
    %table.tableForm
      %tr
        %td.columnLabel
          = f.label :nombre, :class => "apsLabel"
        %td.columnData
          = f.text_field :nombre, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :apellido, :class => "apsLabel"
        %td.columnData
          = f.text_field :apellido, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :document_type_id, :class => "apsLabel"
        %td.columnData
          = f.collection_select :document_type_id, DocumentType.all, :id, :detalle, {:class => "apsSelect"}, {:include_blank => 'Sin datos'}
      %tr
        %td.columnLabel
          = f.label :numero_documento, :class => "apsLabel"
        %td.columnData
          = f.text_field :numero_documento, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :employee_id, :class => "apsLabel"
        %td.columnData
          = f.text_field :employee_id, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :fecha_nacimiento, :class => "apsLabel"
        %td.columnData
          = f.text_field :fecha_nacimiento, :value => @insurance_beneficiary.fecha_nacimiento.to_s, :class => "apsDatetime"
      %tr
        %td.columnLabel
          = f.label :address, :class => "apsLabel"
        %td.columnData
          = f.text_field :address, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :kinship_id, :class => "apsLabel"
        %td.columnData
          = f.collection_select :kinship_id, Kinship.all, :id, :name, {:class => "apsSelect"}, {:include_blank => 'Sin datos'}
      %tr
        %td.columnLabel
          = f.label :porcentual, :class => "apsLabel"
        %td.columnData
          = f.text_field :porcentual, :class => "apsTextbox"
  .actions
    = f.submit :class => "btn success"
