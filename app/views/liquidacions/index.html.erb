<h1>Listing liquidacions</h1>

<table>
  <tr>
    <th>Periodo</th>
    <th>Quincena</th>
    <th>Tipo Recibo</th>
    <th>Fecha liquidacion</th>
    <th>Banco ultimo deposito</th>
    <th>Periodo deposito</th>
    <th>Fecha deposito</th>
    <th>Fecha cierre</th>
    <th>Techo retenciones</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @liquidacions.each do |liquidacion| %>
  <tr>
    <td><%= liquidacion.periodo.year.to_s + '/' + liquidacion.periodo_deposito.month.to_s %></td>
    <td><%= liquidacion.quincena %></td>
    <td><%= liquidacion.tipo_recibo.try(:detalle) %></td>
    <td><%= liquidacion.fecha_liquidacion %></td>
    <td><%= liquidacion.bank_deposit.try(:detalle) %></td>
    <td><%= liquidacion.periodo_deposito.year.to_s + '/' + liquidacion.periodo_deposito.month.to_s %></td>
    <td><%= liquidacion.fecha_deposito %></td>
    <td><%= liquidacion.fecha_cierre %></td>
    <td><%= liquidacion.techo_retenciones %></td>
    <td><%= link_to 'Visualizar', liquidacion %></td>
    <td><%= link_to 'Editar', edit_liquidacion_path(liquidacion) %></td>
    <td><%= link_to 'Borrar', liquidacion, :confirm => 'Esta Seguro?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nueva Liquidacion', new_liquidacion_path %>
