<h1>User Role</h1>
<table class="data" style="width:50%">
	<tr>
		<td style="width:30%;background-color: #C3D5D5;"><strong>User:</strong></td>
		<td style="width:70%"><%= @user_role.user_id %></td>
	</tr>
	<tr>
		<td style="width:30%;background-color: #C3D5D5;"><strong>Rol:</strong></td>
		<td style="width:70%"><%= @user_role.rol_id %></td>
	</tr>
	<tr>
		<td style="width:30%;background-color: #C3D5D5;"><strong>Is Active:</strong></td>
		<td style="width:70%"><%= @user_role.is_active %></td>
	</tr>
</table>
<p>
	<%= link_to "Editar", edit_user_role_path(@user_role) %> |
	<%= link_to "Eliminar", @user_role, :confirm => 'Esta seguro?', :method => :delete %> |
	<%= link_to "User Roles", user_roles_path %>
</p>
