<%= form_for @group_remunerations_remunerative_concept do |f| %>
	<% if @group_remunerations_remunerative_concept.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(@group_remunerations_remunerative_concept.errors.count, "error") %> prohibido guardar Group Remunerations Remunerative Concept:</h2>
			<ul>
			<% @group_remunerations_remunerative_concept.errors.full_messages.each do |msg| %>
				<li><%= msg %></li>
			<% end %>
			</ul>
		</div>
	<% end %>
	<table class="data" style="width:100%" >
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :group_remuneration_id %></strong></td>
			<td style="width:70%">
				<%= f.collection_select :group_remuneration_id, GroupRemuneration.all, :id, :detalle, :include_blank => 'Sin datos' %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :remunerative_concept_id %></strong></td>
			<td style="width:70%">
				<%= f.collection_select :remunerative_concept_id, RemunerativeConcept.all, :id, :detalle, :include_blank => 'Sin datos' %>
			</td>
		</tr>
	</table>
	<p><%= f.submit %></p>
<% end %>
