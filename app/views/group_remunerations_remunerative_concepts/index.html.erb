<h1>Group Remunerations Remunerative Concepts</h1>

<table class="tableStandard">
	<tr>
		<th><%= sort_link @search, :group_remuneration_id, "Group Remuneration" %></td>
		<th><%= sort_link @search, :remunerative_concept_id, "Remunerative Concept" %></td>
		<th colspan='3' style="text-align:center;">Acciones</th>
	</tr>
	<% for group_remunerations_remunerative_concept in @group_remunerations_remunerative_concepts %>
		<tr>
			<td><%= group_remunerations_remunerative_concept.group_remuneration_id %></td>
			<td><%= group_remunerations_remunerative_concept.remunerative_concept_id %></td>
            <td><a href="/group_remunerations_remunerative_concepts/<%= group_remunerations_remunerative_concept.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/group_remunerations_remunerative_concepts/<%= group_remunerations_remunerative_concept.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/group_remunerations_remunerative_concepts/<%= group_remunerations_remunerative_concept.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nuevo Group Remunerations Remunerative Concept", new_group_remunerations_remunerative_concept_path %></p>

<%= paginate @group_remunerations_remunerative_concepts %>
