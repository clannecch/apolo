= form_for(@group_remunerations_remunerative_concept) do |f|
  = render 'shared/errorhandler', :obj_errors => @group_remunerations_remunerative_concept.errors
  .clear
  .form_box
    .form_box_title
    /  %span.title= GroupRemunerationsRemunerativeConcept.model_name.human
    %table.tableForm
      %tr
        %td.columnLabel
          = f.label :group_remuneration_id, :class => "apsLabel"
        %td.columnData
          = f.collection_select :group_remuneration_id, GroupRemuneration.all, :id, :detalle, {:class => "apsSelect"}, {:include_blank => 'Sin datos'}
      %tr
        %td.columnLabel
          = f.label :remunerative_concept_id, :class => "apsLabel"
        %td.columnData
          = f.collection_select :remunerative_concept_id, RemunerativeConcept.all, :id, :detalle, {:class => "apsSelect"}, {:include_blank => 'Sin datos'}
  .actions
    = f.submit :class => "btn success"
