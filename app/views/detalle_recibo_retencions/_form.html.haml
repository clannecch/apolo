= form_for(@detalle_recibo_retencion) do |f|
  - if @detalle_recibo_retencion.errors.any?
    #error_explanation
      %h2
        = pluralize(@detalle_recibo_retencion.errors.count, "error")
        prohibited this detalle_recibo_retencion from being saved:
      %ul
        - @detalle_recibo_retencion.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :recibo_sueldo_id
    %br/
    = f.collection_select :recibo_sueldo_id, ReciboSueldo.all, :id, :employee_id, :prompt => true
  .field
    = f.label :retention_concept_id
    %br/
    = f.collection_select :retention_concept_id, RetentionConcept.all, :id, :detalle, :prompt => true
  .field
    = f.label :detalle
    %br/
    = f.text_field :detalle
  .field
    = f.label :cost_center_id
    %br/
    = f.collection_select :cost_center_id, CostCenter.all, :id, :detalle, :prompt => true
  .field
    = f.label :cantidad
    %br/
    = f.text_field :cantidad
  .field
    = f.label :importe
    %br/
    = f.text_field :importe
  .field
    = f.label :total
    %br/
    = f.text_field :total
  .actions
    = f.submit
