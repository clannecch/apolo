<h1>Cuentas contables</h1>

<table class="tableStandard">
	<tr>
		<th><%= sort_link @search, :detalle, "Detalle" %></td>
		<th colspan='3' style="text-align:center;">Acciones</th>
	</tr>
	<% for accounting_imputation in @accounting_imputations %>
		<tr>
			<td><%= accounting_imputation.detalle %></td>
            <td><a href="/accounting_imputations/<%= accounting_imputation.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/accounting_imputations/<%= accounting_imputation.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/accounting_imputations/<%= accounting_imputation.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nueva Cuenta contable", new_accounting_imputation_path %></p>

<%= paginate @accounting_imputations %>
