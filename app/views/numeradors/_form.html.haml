= form_for(@numerador) do |f|
  - if @numerador.errors.any?
    #error_explanation
      %h2
        = pluralize(@numerador.errors.count, "error")
        prohibited this numerador from being saved:
      %ul
        - @numerador.errors.full_messages.each do |msg|
          %li= msg
  %table.tableForm
    %tr
      %td.columnLabel
        = f.label :code, :class => "apsLabel"
      %td.columnData
        = f.text_field :code, :class => "apsTextbox"
    %tr
      %td.columnLabel
        = f.label :name, :class => "apsLabel"
      %td.columnData
        = f.text_field :name, :class => "apsTextbox"
    %tr
      %td.columnLabel
        = f.label :number, :class => "apsLabel"
      %td.columnData
        = f.text_field :number, :class => "apsTextbox"
  .actions
    = f.submit :class => "btn success"
