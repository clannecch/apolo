%h1 Listing employer_contribution_concepts
%table
  %tr
    %th Codigo
    %th Detalle
    %th Formula de Calculo
    %th Acumuladores
    %th Prioridad
    %th Statistical value
    %th Accounting imputation
    %th
    %th
    %th
  - @employer_contribution_concepts.each do |employer_contribution_concept|
    %tr
      %td= employer_contribution_concept.codigo
      %td= employer_contribution_concept.detalle
      %td= employer_contribution_concept.formula_calculo_valor
      %td= employer_contribution_concept.acumuladores_valor
      %td= employer_contribution_concept.prioridad
      %td= employer_contribution_concept.statistical_value
      %td= employer_contribution_concept.accounting_imputation_id
      %td= link_to 'Show', employer_contribution_concept
      %td= link_to 'Edit', edit_employer_contribution_concept_path(employer_contribution_concept)
      %td= link_to 'Destroy', employer_contribution_concept, :confirm => 'Are you sure?', :method => :delete
%br/
= link_to 'New Employer contribution concept', new_employer_contribution_concept_path
