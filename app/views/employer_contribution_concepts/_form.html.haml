= form_for(@employer_contribution_concept) do |f|
  = render 'shared/errorhandler', :obj_errors => @employer_contribution_concept.errors
  .clear
  .form_box
    .form_box_title
    /  %span.title= EmployerContributionConcept.model_name.human
    %table.tableForm
      %tr
        %td.columnLabel
          = f.label :codigo, :class => "apsLabel"
        %td.columnData
          = f.text_field :codigo, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :detalle, :class => "apsLabel"
        %td.columnData
          = f.text_field :detalle, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :statistical_value, :class => "apsLabel"
        %td.columnData
          = f.text_field :statistical_value, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :accounting_imputation_id, :class => "apsLabel"
        %td.columnData
          = f.collection_select :accounting_imputation_id, AccountingImputation.all, :id, :detalle, {:class => "apsSelect"}, {:include_blank => 'Sin datos'}
      %tr
        %td.columnLabel
          = f.label :prioridad, :class => "apsLabel"
        %td.columnData
          = f.text_field :prioridad, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :acumuladores_valor, :class => "apsLabel"
        %td.columnData
          = f.text_field :acumuladores_valor, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :formula_calculo_valor, :class => "apsLabel"
        %td.columnData
          = f.text_field :formula_calculo_valor, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :retention_concept_id, :class => "apsLabel"
        %td.columnData
          = f.collection_select :retention_concept_id, RetentionConcept.all, :id, :detalle, {:class => "apsSelect"}, {:include_blank => 'Sin datos'}
      %tr
        %td.columnLabel
          = f.label :additional_health_insurance_id, :class => "apsLabel"
        %td.columnData
          = f.text_field :additional_health_insurance_id, :class => "apsTextbox"
      %tr
        %td.columnLabel
          = f.label :health_insurance_id, :class => "apsLabel"
        %td.columnData
          = f.collection_select :health_insurance_id, HealthInsurance.all, :id, :detalle, {:class => "apsSelect"}, {:include_blank => 'Sin datos'}
  .actions
    = f.submit :class => "btn success"
