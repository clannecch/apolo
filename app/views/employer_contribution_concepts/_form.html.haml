= form_for(@employer_contribution_concept) do |f|
  - if @employer_contribution_concept.errors.any?
    #error_explanation
      %h2
        = pluralize(@employer_contribution_concept.errors.count, "error")
        prohibited this employer_contribution_concept from being saved:
      %ul
        - @employer_contribution_concept.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :codigo
    %br/
    = f.text_field :codigo
  .field
    = f.label :detalle
    %br/
    = f.text_field :detalle
  .field
    = f.label :formula_calculo_valor
    %br/
    = f.text_field :formula_calculo_valor
  .field
    = f.label :acumuladores_valor
    %br/
    = f.text_field :acumuladores_valor
  .field
    = f.label :prioridad
    %br/
    = f.text_field :prioridad
  .field
    = f.label :statistical_value
    %br/
    = f.text_field :statistical_value
  .field
    = f.label :accounting_imputation_id
    %br/
    = f.text_field :accounting_imputation_id
  .actions
    = f.submit
