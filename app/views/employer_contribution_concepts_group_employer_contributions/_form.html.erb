<%= form_for @employer_contribution_concepts_group_employer_contribution do |f| %>
	<% if @employer_contribution_concepts_group_employer_contribution.errors.any? %>
		<div id="error_explanation">
			<h2><%= pluralize(@employer_contribution_concepts_group_employer_contribution.errors.count, "error") %> prohibido guardar Employer Contribution Concepts Group Employer Contribution:</h2>
			<ul>
			<% @employer_contribution_concepts_group_employer_contribution.errors.full_messages.each do |msg| %>
				<li><%= msg %></li>
			<% end %>
			</ul>
		</div>
	<% end %>
	<table class="data" style="width:100%" >
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :group_employer_contribution_id %></strong></td>
			<td style="width:70%">
				<%= f.collection_select :group_employer_contribution_id, GroupEmployerContribution.all, :id, :detalle, :include_blank => 'Sin datos' %>
			</td>
		</tr>
		<tr>
			<td style="width:30%;background-color: #C3D5D5;"><strong><%= f.label :employer_contribution_concept_id %></strong></td>
			<td style="width:70%">
				<%= f.collection_select :employer_contribution_concept_id, EmployerContributionConcept.all, :id, :detalle, :include_blank => 'Sin datos' %>
			</td>
		</tr>
	</table>
	<p><%= f.submit %></p>
<% end %>
