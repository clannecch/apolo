= form_for(@issued) do |f|
  - if @issued.errors.any?
    #error_explanation
      %h2
        = pluralize(@issued.errors.count, "error")
        prohibited this issued from being saved:
      %ul
        - @issued.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :detalle
    %br/
    = f.text_field :detalle
  .actions
    = f.submit
