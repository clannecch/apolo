<h1>Listing remunerative_concepts</h1>

<table>
  <tr>
    <th>Codigo</th>
    <th>Detalle</th>
    <th>Porcentual asistencia</th>
    <th>Prioridad calculot</th>
    <th>Datos a Pedir</th>
    <th>Statistical value</th>
    <th>Grupo ganancias</th>
    <th>Accounting imputation</th>
    <th>Concepto asociado haber</th>
    <th>Concepto asociado retencion</th>
    <th>Concepto asociado haber 2</th>
    <th>Concepto asociado retencion 2</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @remunerative_concepts.each do |remunerative_concept| %>
  <tr>
    <td><%= remunerative_concept.codigo %></td>
    <td><%= remunerative_concept.detalle %></td>
    <td><%= remunerative_concept.porcentual_asistencia %></td>
    <td><%= remunerative_concept.prioridad_calculo %></td>
    <td><%= remunerative_concept.data_to_ask_id %></td>
    <td><%= remunerative_concept.statistical_value %></td>
    <td><%= remunerative_concept.grupo_ganancias_id %></td>
    <td><%= remunerative_concept.accounting_imputation_id %></td>
    <td><%= remunerative_concept.concepto_asociado_haber   %></td>
    <td><%= remunerative_concept.concepto_asociado_retencion %></td>
    <td><%= remunerative_concept.concepto_asociado_haber_2 %></td>
    <td><%= remunerative_concept.concepto_asociado_retencion_2 %></td>
    <td><%= link_to 'Show', remunerative_concept %></td>
    <td><%= link_to 'Edit', edit_remunerative_concept_path(remunerative_concept) %></td>
    <td><%= link_to 'Destroy', remunerative_concept, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

