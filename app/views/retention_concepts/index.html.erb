<h1>Codigos de Retenciones</h1>

<table class="tableStandard">
	<tr>
		<th><%= sort_link @search, :codigo, "Codigo" %></td>
		<th><%= sort_link @search, :detalle, "Detalle" %></td>
		<th><%= sort_link @search, :statistical_value, "Statistical Value" %></td>
		<th><%= sort_link @search, :accounting_imputation_id, "Accounting Imputation" %></td>
		<th><%= sort_link @search, :prioridad, "Prioridad" %></td>
		<th><%= sort_link @search, :formula_calculo_valor, "Formula Calculo Valor" %></td>
		<th><%= sort_link @search, :acumuladores_valor, "Acumuladores Valor" %></td>
		<th><%= sort_link @search, :data_to_ask_id, "Data To Ask" %></td>
		<th colspan='3' style="text-align:center;">Acciones</th>
	</tr>
	<% for retention_concept in @retention_concepts %>
		<tr>
			<td><%= retention_concept.codigo %></td>
			<td><%= retention_concept.detalle %></td>
			<td><%= retention_concept.statistical_value %></td>
			<td><%= retention_concept.accounting_imputation_id %></td>
			<td><%= retention_concept.prioridad %></td>
			<td><%= retention_concept.formula_calculo_valor %></td>
			<td><%= retention_concept.acumuladores_valor %></td>
			<td><%= retention_concept.data_to_ask_id %></td>
            <td><a href="/retention_concepts/<%= retention_concept.id.to_s %>"><img alt="Ver" src="images/icons/16x16/application_get.png" title="Ver" /></a></td>
            <td><a href="/retention_concepts/<%= retention_concept.id.to_s %>/edit"><img alt="Editar" src="images/icons/16x16/application_edit.png" title="Editar" /></a></td>
            <td><a href="/retention_concepts/<%= retention_concept.id.to_s %>" data-confirm="Esta ud. seguro?" data-method="delete" rel="nofollow">
				<img alt="Eliminar" src="images/icons/16x16/application_delete.png" title="Eliminar" /></a>
			</td>
		</tr>
	<% end %>
</table>
<p><%= link_to "Nuevo Codigo de Retencion", new_retention_concept_path %></p>

<%= paginate @retention_concepts %>
