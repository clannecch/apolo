<% @group_retentions.each do |group_retention| %>
  <tr>
    <td><%= group_retention.detalle %></td>
    <td><%= link_to 'Visualizar', group_retention %></td>
    <td><%= link_to 'Editar', edit_group_retention_path(group_retention) %></td>
