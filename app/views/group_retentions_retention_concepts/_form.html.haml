= form_for(@group_retentions_retention_concept) do |f|
  = render 'shared/errorhandler', :obj_errors => @group_retentions_retention_concept.errors
  .clear
  .form_box
    .form_box_title
    /  %span.title= GroupRetentionsRetentionConcept.model_name.human
    %table.tableForm
      %tr
        %td.columnLabel
          = f.label :group_retention_id, :class => "apsLabel"
        %td.columnData
          = f.collection_select :group_retention_id, GroupRetention.all, :id, :detalle, {:class => "apsSelect"}, {:include_blank => 'Sin datos'}
      %tr
        %td.columnLabel
          = f.label :retention_concept_id, :class => "apsLabel"
        %td.columnData
          = f.collection_select :retention_concept_id, RetentionConcept.all, :id, :detalle, {:class => "apsSelect"}, {:include_blank => 'Sin datos'}
  .actions
    = f.submit :class => "btn success"
