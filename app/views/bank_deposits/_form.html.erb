<%= form_for @bank_deposit do |f| %>
  <% if @bank_deposit.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@bank_deposit.errors.count, "error") %> prohibido guardar Banco:</h2>
      <ul>
        <% @bank_deposit.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <table class="data" style="width:100%" >
    <tr>
      <td class="labelCaption" style="width:30%"><%= f.label :detalle %></td>
      <td class="labelData" style="width:70%">
        <%= f.text_field :detalle, {:class => "textbox"} %>
      </td>
    </tr>
  </table>
  <p><%= f.submit %></p>
<% end %>
